DynamicSureMin=function(x,v,d=10){#
    n <- length(x)#
    a=matrix(rep(0,n*n),ncol=n) ##separation#
	b=a ##value#
	for (i in 1:n-d+1){#
		j=i+d-1#
		a[i,j]=j#
		b[i,j]=sure.spher(x[i:j], v[i:j])#
	}#
	for (l in d:(n-d+1)){#
		if (l %% 20==0){#
		    print(l)#
		}#
		for (i in 1:(n-l)){#
			j=l+i#
			sure=sure.spher(x[i:j], v[i:j])#
		#	print(sure)#
			a[i,j]=j#
			if ((i+d-1)<=(j-d)){#
			for (k in (i+d-1):(j-d)){#
				#temp=sure.spher(x[i:k], v[i:k])+sure.spher(x[(k+1):j], v[(k+1):j])#
				temp=b[i,k]+b[k+1,j]#
				if (sure>temp){#
					sure=temp#
				#	print(sure)#
					a[i,j]=k#
					b[i,j]=sure#
				}}#
			}#
		}#
	}#
	list(a,b)#
}
