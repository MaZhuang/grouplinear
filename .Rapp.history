    n <- length(x)#
    a=matrix(rep(0,n*n),ncol=n) ##separation#
	b=a ##value#
	for(i in 1:n){#
		a[i,i]=i#
		b[i,i]=v[i]#
	}#
	for (l in 1:(n-1)){#
		if (l %% 20==0){#
		    print(l)#
		}#
		for (i in 1:(n-l)){#
			j=l+i#
			sure=sure.spher(x[i:j], v[i:j])#
		#	print(sure)#
			a[i,j]=j#
